@*View file returns the data from repository to user*@
@model IEnumerable<HR_Repository_Cached_Services.Models.JobModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Hackerrank Cache Service</title>
</head>
<body>
   <h4> Hackerrank Cached Services</h4>
    <h4> I've solved this Problem.</h4>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Job_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustomerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Location)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TechniciansAssigned)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estimate)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Job_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CustomerName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Location)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TechniciansAssigned)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Estimate)
                </td>
                
            </tr>
        }

    </table>
</body>
</html>
